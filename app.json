{
  "expo": {
    "name": "TREND_mobile",
    "slug": "TREND_mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/TREND/trend.webp",
    "scheme": "trendmobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UILaunchScreen": {
          "UIColorName": "SplashScreenBackground",
          "UIImageName": "SplashScreenLogo"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/TREND/trend.webp",
        "backgroundColor": "#FFFFFF"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/TREND/trend.webp",
          "imageWidth": 220,
          "resizeMode": "contain",
          "backgroundColor": "#FFFFFF",
          "dark": {
            "image": "./assets/images/TREND/trend.webp",
            "backgroundColor": "#1a1a1a"
          }
        }
      ],
      "expo-web-browser",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "splash": {
      "image": "./assets/images/TREND/trend.webp",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    }
  }
}